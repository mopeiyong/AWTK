<dialog anim_hint="center_scale" self_layout="default(x=66,y=78,w=347,h=115)" >
    <dialog_title h="20" tr_text="Timing Setting" w="100%" x="0" y="0"/>
    <dialog_client h="-20" w="100%" x="0" y="bottom">
        <row h="17" layout="row:1 col:3" w="120" x="17" y="6">
            <label style="timing" tr_text="hour"/>
            <label style="timing" tr_text="minute"/>
            <label style="timing" tr_text="second"/>
        </row>
        <row h="68" layout="row:1 col:3" w="120" x="18" y="23">
            <text_selector name="hour_selector" options="1-24" text="15" visible_nr="3"/>
            <text_selector name="min_selector"  options="1-60" text="10" visible_nr="3"/>
            <text_selector name="sec_selector"  options="1-60" text="16" visible_nr="3"/>
        </row>
        <button h="20" name="ok" style="timing_switch_btn" tr_text="On" w="90" x="right:18" y="18"/>
        <button h="20" name="cancle" style="timing_switch_btn" tr_text="Off" w="90" x="right:18" y="bottom:18"/>
    </dialog_client>
</dialog> 