<alarm_text_selector>
    <style name="alarm_select" fg_color="#ffffff" text_color="white" text_align_h="center" font_size="36" >
        <normal bg_color="#000000" border_color="#000000" />
    </style>
    <!--<style name="select" fg_color="#ffffff" text_color="white" text_align_h="center" font_size="36" ><normal bg_color="#000000" border_color="#6079FF" /></style>-->
    <!--<style name="no_select" fg_color="#ffffff" text_color="white" text_align_h="center" font_size="36" ><normal bg_color="#000000" border_color="#000000" /></style>-->
    <style name="select" fg_color="#ffffff" text_color="white" text_align_h="center" font_size="36" >
        <normal bg_color="black" border_color="#6079FF" />
    </style>
</alarm_text_selector>
<button>
    <style name="music" font_name="default" text_color="#FFFFFF" >
        <normal     bg_color="#4B4B4B"/>
        <pressed    bg_color="#787878"/>
        <over       bg_color="#5c5c5c"/>
    </style>
    <style name="white" text_align_h="center" text_align_v="middle" text_color="white"
         border_color="">
        <normal text_color="white"/>
    </style>
</button>
<view>
    <style name="select" fg_color="#ffffff" text_color="white" text_align_h="center" font_size="36" >
        <normal bg_color="#000000" border_color="#6079FF" round_radius="10"/>
    </style>
    <style name="no_select" fg_color="#ffffff" text_color="white" text_align_h="center" font_size="36" >
        <normal bg_color="#000000" border_color="#333333" round_radius="10"/>
    </style>
</view>
<text_selector>
    <style name="hour_selector">
        <normal font_size="10"/>
    </style>
</text_selector>
