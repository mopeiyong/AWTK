<window style="watch_06" full_screen="false" self_layout="default(x=205,y=45,w=390,h=390)" children_layout="default(r=1,c=1)" theme="watch_06_styles">
    <gesture name="gesture" x="c" y="m" w="100%" h="100%">
        <label name="minute" x="57" y="160" w="15" h="25" text="2" style="heart_number"/>
        <label x="77" y="166" w="58" h="19" text="分钟前" style="heart_text"/>
        <label name="number" x="150" y="160" w="36" h="25" text="68" style="heart_number"/>
        <label x="190" y="166" w="47" h="21" text="次/分" style="heart_text"/>
        <row x="151" y="61" w="165" h="77">
            <digit_clock x="right" y="0" w="100%" h="52" format="hh:mm" style="time"/>
            <column x="0" y="63" w="100%" h="17">
                <digit_clock x="84" y="m" w="37" h="17" format="M/D" style="date"/>
                <label name="wday" x="right" y="m" w="36" h="16" text="周五" style="week"/>
            </column>
        </row>
        <bar_graph x="c" y="196" w="274" h="53" name="heart_rate_bar" size="15" spacing="4" color="#FF0138" arc="true"/>
        <button name="weather" style="watch_06_weather_00" enable_long_press="true" x="55" y="51" w="88" h="88" />
        <button name="calendar" style="watch_06_calendar" enable_long_press="true" x="59" y="257" w="88" h="88" />
        <button name="health" style="watch_06_health" enable_long_press="true" x="150" y="287" w="88" h="88"/>
        <button name="music" style="watch_06_music" enable_long_press="true" x="242" y="258" w="88" h="88" />
    </gesture>
</window>