<window>
    <style name="watch_06">
        <normal bg_image="ico_watch_background_06"/>
    </style>
</window>
<watch_slide_view>
    <style name="default">
        <normal  fg_color="red" />
    </style>
    <style name="dot">
        <normal  icon="dot" active_icon="active_dot"/>
    </style>
</watch_slide_view>
<digit_clock>
    <style name="time">
        <normal text_color="#26ffed" font_size="55" text_align_h="right" text_align_v="middle" />
    </style>
    <style name="date">
        <normal text_color="#26ffed" font_size="17" text_align_h="center" text_align_v="middle" />
    </style>
</digit_clock>
<label>
    <style name="heart_number">
        <normal text_color="#26ffed" font_size="32" text_align_h="center" text_align_v="middle" />
    </style>
    <style name="heart_text">
        <normal text_color="#999999" font_size="18" text_align_h="center" text_align_v="middle" />
    </style>
    <style name="week">
        <normal text_color="#26ffed" font_size="17" text_align_h="right" text_align_v="middle" />
    </style>
</label>
<button>
