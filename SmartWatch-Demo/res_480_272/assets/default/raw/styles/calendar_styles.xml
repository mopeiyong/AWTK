        <pressed     bg_image="ico_up_pressed" />
        <over        bg_image="ico_up_pressed" />
    </style>
    <style name="current">
        <normal      bg_image="calendar_current" />
        <pressed     bg_image="calendar_current" />
        <over        bg_image="calendar_current" />
    </style>
    <style name="down">
        <normal      bg_image="ico_down_normal" />
        <pressed     bg_image="ico_down_pressed" />
        <over        bg_image="ico_down_pressed" />
    </style>
    <style name="alarmitem_bg"  >
        <normal      bg_image="alarmitem_bg" />
        <pressed     bg_image="alarmitem_bg" />
        <over        bg_image="alarmitem_bg" />
